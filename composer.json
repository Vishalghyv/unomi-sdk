{
  "name": "dropsolid/unomi-sdk-php",
  "description": "PHP client to connect to the Unomi API ",
  "type": "library",
  "authors": [
    {
      "name": "Nick Veenhof",
      "role": "Lead Developer"
    }
  ],
  "keywords": [
    "Unomi",
    "api"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "league/oauth2-client": "^2.4",
    "php-http/httplug": "^1.1|^2.0",
    "psr/http-message": "^1.0",
    "php-http/client-implementation": "^1.0|^2.0",
    "php-http/message-factory": "^1.0",
    "php-http/discovery": "^1.0",
    "php-http/client-common": "^1.0|^2.0",
    "symfony/serializer": "^3.0|^4.0|^5.0"
  },
  "require-dev": {
    "phpspec/phpspec": "^6.1",
    "php-http/mock-client": "^1.3",
    "php-http/message": "^1.8",
    "guzzlehttp/psr7": "^1.6",
    "symfony/var-dumper": "^3.0|^4.0|^5.0",
    "namshi/cuzzle": "^2.0",
    "symfony/serializer": "^3.0|^4.0|^5.0",
    "php-http/guzzle6-adapter": "^1.0",
    "dropsolid/oauth2-dropsolid-platform": "dev-master"
  },
  "suggest": {
    "symfony/serializer": "^5.0",
    "php-http/guzzle6-adapter": "^1.0",
    "dropsolid/oauth2-dropsolid-platform": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Dropsolid\\UnomiSdkPhp\\": [
        "src/"
      ]
    }
  },
  "config": {
    "bin-dir": "bin"
  }
}
